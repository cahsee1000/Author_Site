<h1 style="text-align: center; padding: 30px"><a href="/">Cassie L Morrison</a></h1>
<ul id="navbar">
  <div id="innernav">
    <% if can? :read, Book %>
      <li class="dropdown">
        <a href="/books" class="dropbtn">Books</a>
        <div class="dropdown-content">
          <% @books.each do |book| %>
            <%=link_to book.title.humanize, book_path(book)%>
          <%end %>
        </div>
      </li>
    <%end %>
    <% if can? :read, Event %>
      <li class="dropdown">
        <a href="/events" class="dropbtn">Events</a>
        <div class="dropdown-content">
          <% @events.each do |event| %>
            <%=link_to event.title.humanize, event_path(event)%>
          <%end %>
        </div>
      </li>
    <%end %>
    <% if can? :read, Blog %>
      <li class="dropdown">
        <a href="/blogs" class="dropbtn">Blogs</a>
        <div class="dropdown-content">
          <% @blogs.each do |blog| %>
            <%=link_to blog.title.humanize, blog_path(blog)%>
          <%end %>
        </div>
      </li>
  <%end %>
  <li><a href='/aboutme'>About</a></li>
  </div>
</ul>